<!-- Plugin files -->
{% for name, assets in assetPlugins.items() %}
    {% if "js" in assets %}
        {% for asset in assets["js"] %}
            <script type="text/javascript" src="{{ url_for('plugin_assets', name=name, filename=asset) }}"></script>
        {% endfor %}
    {% endif %}
{% endfor %}
